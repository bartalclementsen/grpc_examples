  "ReverseProxy": {
    "Routes": {
      "minimumroute": {
        "ClusterId": "minimumcluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },

    "Clusters": {
      "minimumcluster": {
        "Destinations": {
          "google.com": {
            "Address": "https://localhost:7044/"
          }
        }
      }
    }
  }
}
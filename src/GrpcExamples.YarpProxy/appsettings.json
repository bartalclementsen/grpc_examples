{
  "Urls": "http://localhost:5000;https://localhost:5001",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "http": {
        "Url": "https://localhost:5001",
        "Protocols": "Http2"
      }
    }
  },
  "AllowedHosts": "*",
